<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="631.0" prefWidth="811.0" stylesheets="@../css/dashboard.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
            <Label fx:id="basicLabel" text="Welcome to dashboard!">dd</Label>
   <top>
      <MenuBar id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu id="menu" mnemonicParsing="false" styleClass="menu" text="%app.dashboard.bug">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#createNew" styleClass="menuItem" text="%app.dashboard.new" />
                  <MenuItem mnemonicParsing="false" styleClass="menuItem" text="%app.dashboard.import" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" styleClass="menu" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" styleClass="menuItem" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" styleClass="menu" text="%app.dashboard.account">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onProfile" styleClass="menuItem" text="%app.dashboard.profile" />
                  <MenuItem mnemonicParsing="false" onAction="#onLogout" styleClass="menuItem" text="%app.dashboard.logout" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <Label fx:id="testLabel" text="Label" underline="true" visible="false" BorderPane.alignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </bottom>
   <center>
      <SplitPane dividerPositions="0.4" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <VBox spacing="20.0">
               <children>
                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Filter:" GridPane.columnSpan="2" GridPane.halignment="CENTER">
                           <font>
                              <Font size="19.0" />
                           </font>
                        </Label>
                     </children>
                  </GridPane>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                      <Button fx:id="fldOk" mnemonicParsing="false" onAction="#onOk" text="OK" />
                    </buttons>
                  </ButtonBar>
               </children>
            </VBox>
            <ScrollPane fitToHeight="true" fitToWidth="true">
               <content>
                  <FlowPane fx:id="flowPane" hgap="10.0" prefHeight="200.0" prefWidth="200.0" vgap="10.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <children>
                     </children>
                  </FlowPane>
               </content>
            </ScrollPane>
         </items>
      </SplitPane>
   </center>
</BorderPane>
